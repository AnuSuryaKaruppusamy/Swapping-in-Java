This is Swapping Model 2 with Addition and Subtraction

package Sample;
public class Swapping {
	public static void main(String[] args) {
        int a=10;
        int b=20;
        a=a+b;
        b=a-b;
        a=a-b;
        System.out.println("Enter the a value:"+a);
        System.out.println("Enter the b value:" +b);        
        }
        }
		
