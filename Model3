This is Model3 with Multiplication,Division and Modulus

package Sample;
public class Swapping {
	public static void main(String[] args) {
    int a=10;
    int b=20;
		a=a*b;
		b=a/b;
		a=a/b;
		System.out.println("Enter the a value:" +a);
		System.out.println("Enter the b value:" +b);       
		
	}

}
